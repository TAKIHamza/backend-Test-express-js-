{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {
          "src": "/auth/(.*)",
          "dest": "/auth/"
        },
        {
          "src": "/product/(.*)",
          "dest": "/product/"
        },
        {
          "src": "/command/(.*)",
          "dest": "/command/"
        },
        {
          "src": "/(.*)",
          "dest": "/$1"
        }
      ]
  }
  